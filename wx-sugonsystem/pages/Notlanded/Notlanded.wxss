/* pages/Notlanded/Notlanded.wxss */
.whole {
  background-color: #fafafa;
}

.user {
  /* width: 375px; */
  height: 400rpx;
  top: 30rpx;
  position: relative;
  text-align: center;
  display: flex;
  justify-content: center;
}
.user image {
  height: 230rpx;
  width: 230rpx;
  border-radius: 50%;
  border: 14rpx solid rgba(252, 249, 249, 0);
}
.word {
  position: absolute;
  top: 320rpx;
  left:180rpx;
}
.t {
  z-index: 3;
  width: 384rpx;
  height: 60rpx;
  color: #203152;
  font-family: "PingFangSC-Regular";
  font-size: 48rpx;
  font-weight: 400;
  line-height: 60rpx;
  text-align: center;
}

.b {
  font-size: 30rpx;
  color: #7c8698;
  font-weight: 400;
  line-height: 18rpx;
  text-align: center;
  margin: 0rpx auto;

}
.form{
  margin-top: 200rpx;
}

.label{
  margin: 40rpx;
}

.int{
  margin:40rpx;
  padding: 15rpx 40rpx;
  border-radius: 50rpx;
  background-color: rgb(241, 243, 247);
  height: 70rpx;
}

.tab {
  height: 80rpx;
  position: relative;
  top: 100rpx;
}

.submit{
  position: relative;
  width: 180rpx !important;
  height: 180rpx !important;
  border-radius: 50%;   /*圆角半径*/
  background-image: linear-gradient(to bottom,rgb(255, 255, 255), rgb(242, 122, 107), rgb(255, 255, 255));
  box-shadow: 0rpx 4rpx 5rpx 4rpx rgba(255, 240, 240, 0.1);
  /* line-height:130rpx; */
  align-items: center;
  justify-content: center;
  font-size: 40rpx;
  font-weight: 250;
  letter-spacing:2rpx;
  -webkit-box-reflect: below 1px -webkit-linear-gradient(transparent, transparent 60%, rgba(255, 255, 255, 0.6));
}

.go{
  display: block;
  line-height: 150rpx;
  font-weight: 700;
  font-size: 60rpx;
  vertical-align: middle;
  color: white
}

.wxLand{
  width: 224rpx;
  height: 70rpx;
}

.inputPasswd{
  margin-bottom: 20rpx;
}
